    "description": "bot for create order for car wash",
    "main": "/src/index.ts",
    "type": "commonjs",
    "engines": {
        "node": "18.x"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
